   <string notr="true">#centralwidget {
	background: white;
}

 /* Boton para vilver al inicio */
#openIndexWindow {
	font-size: 16pt;
	background: whitesmoke;
	border-radius: 15px;
	border: 1px solid black;
}

#openIndexWindow:hover {
	color: white;
	width: 20px;
	background-color: blue;
}

/* Boton para abrir dialogo de configuracion de entrenamiento */
#openTrainingConfig {
	font-size: 12pt;
	background: whitesmoke;
	border-radius: 10px;
	border: 1px solid black;
}

#openTrainingConfig:hover {
	background: white;
	border: 2px solid blue;
}

/* Boton para comenzar entrenamiento */
#startTraining {
	font-size: 12pt;
	background: whitesmoke;
	border-radius: 10px;
	border: 1px solid black;
}

#startTraining:hover {
	background: green;
	color: white;
}

/* Boton para detener entrenamiento */
#stopTraining {
	font-size: 12pt;
	background: whitesmoke;
	border-radius: 10px;
	border: 1px solid black;
}

#stopTraining:hover {
	background: red;
	color: white;
}</string>
